{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-pdo": "*",
        "daniel-de-wit/lighthouse-sanctum": "^2.1.1",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jackiedo/dotenv-editor": "^2.0",
        "james-heinrich/getid3": "^1.9",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^9.11",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.7",
        "mll-lab/graphql-php-scalars": "^5.4",
        "nuwave/lighthouse": "^5.48",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-permission": "^5.5",
        "ext-dom": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mll-lab/laravel-graphql-playground": "^2.6",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.20",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lighthouse:ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "install-shoutzor": [
            "@composer install --optimize-autoloader --no-dev",
            "@php artisan key:generate",
            "@php artisan shoutzor:install --useenv",
            "npm ci",
            "npm run production",
            "@composer optimize-shoutzor"
        ],
        "install-shoutzor-dev": [
            "@composer install --optimize-autoloader",
            "@php artisan key:generate",
            "@php artisan shoutzor:install --useenv --dev",
            "npm ci",
            "npm run dev",
            "@composer optimize-shoutzor"
        ],
        "optimize-shoutzor": [
            "@php artisan optimize",
            "@php artisan view:cache"
        ],
        "add-mock-data": [
            "@php artisan db:seed --class=DevelopmentSeeder",
            "@composer optimize-shoutzor"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
